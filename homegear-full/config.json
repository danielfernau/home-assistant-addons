{
  "name": "Homegear (full)",
  "version": "0.8",
  "slug": "homegear-full",
  "description": "Homegear as a Hassio add-on",
  "arch": [
    "armv7",
    "armhf",
    "aarch64"
  ],
  "startup": "services",
  "boot": "auto",
  "webui": "http://[HOST]:[PORT:2001]",
  "auto_uart": true,
  "gpio": true,
  "image": "danielfernau/hassio-homegear-full-{arch}",
  "privileged": [
    "SYS_ADMIN",
    "SYS_RAWIO"
  ],
  "map": [
    "config:rw",
    "share:rw"
  ],
  "ports": {
    "2001/tcp": 2001,
    "2002/tcp": 2002,
    "2003/tcp": 2003
  },
  "environment": {},
  "options": {},
  "schema": {},
  "devices": [
    "/dev/ttyAMA0:/dev/ttyAMA0:rwm",
    "/dev/ttyACM0:/dev/ttyACM0:rwm",
    "/dev/spidev0.0:/dev/spidev0.0:rwm"
  ]
}